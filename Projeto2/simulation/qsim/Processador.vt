module BANCOdeREGISTRADORES32bits_vlg_sample_tst(
	CLK,
	D,
	ENA,
	sampler_tx
);
input  CLK;
input [31:0] D;
input  ENA;
output sampler_tx;

reg sample;
time current_time;
always @(CLK or D or ENA)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
